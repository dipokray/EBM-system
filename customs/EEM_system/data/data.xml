<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data >
        <record id="seq_eem_pre" model="ir.sequence">
            <field name="name">EEM Pre-costing Sequence</field>
            <field name="code">eem.pre_costing</field><!--code_by_next-->
            <field name="prefix">EEM/%(year)s/%(month)s-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
    <data>
        <record id="seq_eem_bill" model="ir.sequence">
            <field name="name">EEM Bill Sequence</field>
            <field name="code">eem.billing</field><!--code_by_next-->
<!--            <field name="prefix">EEM/%(year)s/</field>-->
            <field name="prefix">Bill/%(year)s/%(month)s-</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>

</odoo>