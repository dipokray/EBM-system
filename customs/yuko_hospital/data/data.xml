<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data >
                       <!--hospital patient sequence-->
        <record id="seq_hospital_patient" model="ir.sequence">
            <field name="name">Patient Sequence</field>
            <field name="code">hospital.patient</field><!--code_by_next-->
            <field name="prefix">HP</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

                       <!--hospital doctor sequence-->

        <record id="seq_hospital_doctor" model="ir.sequence">
            <field name="name">Doctor Sequence</field>
            <field name="code">hospital.doctor</field> <!--code_by_next-->
            <field name="prefix">HD</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_hospital_appointment" model="ir.sequence">
            <field name="name">Appointment Sequence</field>
            <field name="code">hospital.appointment</field> <!--code_by_next-->
            <field name="prefix">OT</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

</odoo>